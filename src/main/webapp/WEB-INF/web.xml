<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <display-name>Login</display-name>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>Logout</display-name>
        <servlet-name>Logout</servlet-name>
        <servlet-class>servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>SignUp</display-name>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>servlets.SignupServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>Index</display-name>
        <servlet-name>Index</servlet-name>
        <servlet-class>servlets.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>BetAjax</display-name>
        <servlet-name>BetAjax</servlet-name>
        <servlet-class>servlets.BetAjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RankingServlet</servlet-name>
        <servlet-class>servlets.RankingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BettorServlet</servlet-name>
        <servlet-class>servlets.BettorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RankingServlet</servlet-name>
        <url-pattern>/ranking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BettorServlet</servlet-name>
        <url-pattern>/bettor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BetAjax</servlet-name>
        <url-pattern>/betajax</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
</web-app>
